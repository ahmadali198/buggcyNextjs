generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String?
  name      String
  age       Int?
  gender    String?
  image     String?
  provider  String   @default("local")
  createdAt DateTime @default(now())
}
